{
    "name": "tpl-ttek/tk004",
    "type": "project",
    "description": "This is a template site with admin area and user login areas.",
    "keywords":  [
        "tropotek",
        "tk004"
    ],
    "time": "2012-12-09",
    "license": "MIT",
    "version": "trunk",
    "minimum-stability": "dev",
    "authors":  [
        {
            "name": "Michael Mifsud",
            "email": "info@tropotek.com.au",
            "homepage": "http://www.tropotek.com/"
        }
    ],
    "autoload":  {
        "psr-0":  {
            "":  [
                "src/",
                "plugin/"
            ]
        }
    },
    "scripts":  {
        "post-install-cmd":  [
            "config\\ComposerEvent::postInstall"
        ],
        "post-update-cmd":  [
            "config\\ComposerEvent::postUpdate"
        ]
    },



    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.kaoses.com/"
        }
